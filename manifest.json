{
  "manifest_version": 3,
  "name": "Gerador de Inputs",
  "version": "1.0.0",
  "description": "Gere dados automaticamente em campos de formul√°rio com um clique",
  "permissions": [
    "contextMenus",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}